<div id="other-cast">
  <h4>Rest of Cast Listed Alphabetically</h4>
  <table class="table table-nonfluid">
    <%= for view <- @other_cast do %>
    <tr>
      <td>
        <%= render_people_link(@conn, view.entity_id, view.names.display_name, view.showcase, view.type) %>
      </td>
      <td>
        <%= for role <- view.roles do %>
          <%= role %><br/>
        <% end %>
      </td>
    </tr>
    <% end %>
  </table>
</div>
